{
  "name": "video-backend",
  "version": "0.1.0",
  "main": "src/index.js",
  "type": "module",
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ]
  },
  "scripts": {
    "dev": "nodemon --watch src -r dotenv/config src/index.js",
    "worker": "node src/services/worker.js",
    "start": "node -r dotenv/config src/index.js",
    "build": "",
    "migrate": "prisma migrate dev --name init",
    "seed": "node -r dotenv/config prisma/seed.js",
    "test": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test:docker": "docker-compose up -d postgres-test-db && docker-compose run --rm app npm run test && docker-compose down -v",
    "lint": "eslint . --ext .js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.0.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "bcrypt": "^5.1.0",
    "bullmq": "^5.66.5",
    "cloudinary": "^2.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-async-errors": "^3.1.1",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^4.0.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.12",
    "socket.io": "^4.8.3",
    "streamifier": "^0.1.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "nodemon": "^2.0.0",
    "prisma": "^5.0.0",
    "supertest": "^6.3.4"
  }
}
